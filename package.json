{
  "name": "transform-stream-tokens",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/transform-stream.js",
  "module": "src/transform-stream.js",
  "description": "Transform stream that emits tokens",
  "keywords": [
    "parser",
    "token"
  ],
  "author": "Markus Felten <markus.felten@gmx.de>",
  "contributors": [
    "Markus Felten <markus.felten@gmx.de>"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/transform-stream.js --section=API",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/transform-stream.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.6",
    "documentation": "^8.0.2",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.0.1",
    "rollup": "^0.63.5",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^15.9.3",
    "split": "^1.0.1",
    "streamss-cat": "^0.1.1",
    "travis-deploy-once": "^5.0.1"
  },
  "engines": {
    "node": ">=10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/transform-stream-tokens.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/transform-stream-tokens/issues"
  },
  "homepage": "https://github.com/arlac77/transform-stream-tokens#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
