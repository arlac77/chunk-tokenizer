{
  "name": "transform-stream-tokens",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "description": "Transform stream that emits tokens",
  "keywords": [
    "npm-package-template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/transform-stream.js --section=API",
    "lint": "documentation lint src/transform-stream.js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "documentation": "^8.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^12.0.2",
    "ava": "^0.25.0",
    "rollup": "^0.60.2",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^15.5.1",
    "travis-deploy-once": "^5.0.0"
  },
  "engines": {
    "node": ">=8.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/transform-stream-tokens.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/transform-stream-tokens/issues"
  },
  "homepage": "https://github.com/arlac77/transform-stream-tokens#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}