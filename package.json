{
  "name": "transform-stream-tokens",
  "version": "0.0.0-semantic-release",
  "description": "Transform stream that emits tokens",
  "keywords": [
    "parser",
    "token"
  ],
  "main": "dist/transform-stream.js",
  "module": "src/transform-stream.js",
  "scripts": {
    "cover": "nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c -- src/transform-stream.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/transform-stream-tokens.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/transform-stream-tokens/issues"
  },
  "homepage": "https://github.com/arlac77/transform-stream-tokens#readme",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.1.0",
    "rollup": "^0.47.4",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "semantic-release": "^6.3.6",
    "split": "^1.0.1",
    "streamss-cat": "^0.1.1",
    "xo": "^0.19.0"
  },
  "contributors": [
    "Markus Felten <markus.felten@gmx.de>"
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.3.0"
  },
  "author": "Markus Felten <markus.felten@gmx.de>",
  "nyc": {
    "include": [
      "build/test-bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "presets": [
      "stage-3"
    ],
    "require": [
      "babel-register"
    ]
  }
}
