{
  "name": "chunk-tokenizer",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/chunk-tokenizer.js",
  "module": "src/chunk-tokenizer.js",
  "description": "Transform stream that emits tokens",
  "keywords": [
    "parser",
    "token"
  ],
  "author": "Markus Felten <markus.felten@gmx.de>",
  "contributors": [
    "Markus Felten <markus.felten@gmx.de>"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/chunk-tokenizer.js --section=API",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "lint": "documentation lint src/chunk-tokenizer.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.0-rc.2",
    "documentation": "^8.1.2",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.67.1",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.12.0",
    "split": "^1.0.1",
    "streamss-cat": "^1.0.0",
    "travis-deploy-once": "^5.0.9",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/chunk-tokenizer.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/chunk-tokenizer/issues"
  },
  "homepage": "https://github.com/arlac77/chunk-tokenizer#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "presets": [],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
